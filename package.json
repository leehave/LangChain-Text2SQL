{
  "name": "langchain-chatbot",
  "version": "1.0.0",
  "description": "LangChainJS + AntDX Chatbot Application",
  "private": true,
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "dev:web": "pnpm --filter web dev",
    "dev:server": "pnpm --filter server dev",
